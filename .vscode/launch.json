{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to Docker (NestJS)",
        "port": 9229,
        "address": "localhost",
        "restart": false,
        "localRoot": "${workspaceFolder}",
        "remoteRoot": "/usr/src/app",
        "protocol": "inspector",
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }